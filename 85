#string functions
a="computer education"
b="MACHILIPATNAM"
c="state bank of india"
d="     india times      "
print(len(a))
print(a.count('i'))
print(a.index('n'))
print(a.upper())
print(b.lower())
print(c.title())
print(a.swapcase())
print(a.casefold())
print(a.capitalize())
print(a.replace('o','x'))
print(d.strip())
print(d.lstrip())
print(d.rstrip())
print(a.zfill(30))
print(a.startswith("bri"))
print(a.endswith("on"))
print(a.find("bank"))
